{
  "base": "semantic/",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "../dist/semantic",
      "uncompressed": "../dist/semantic/components/",
      "compressed": "../dist/semantic/components/",
      "themes": "../dist/semantic/themes/"
    },
    "clean": "../dist/semantic/"
  },
  "components": ["sidebar", "transition", "embed", "dropdown", "popup", "modal", "dimmer"],
  "permission": false,
  "autoInstall": false,
  "rtl": false,
  "version": "2.3.3"
}